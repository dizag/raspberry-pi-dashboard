<!DOCTYPE html>
<html>
  <head>
		<meta http-equiv="refresh" content="6">
    <title>node-server</title>
		<style>
			p {font-size : 12pt;}
			
			div.column-left   { width: 20%px; float: left; }
			div.column-main   { width: 80%px; height: 1000px; }
			
 		</style>
		
  </head>
  <body>
		
		<div>
			<div class="column-left" style="float: left; width: 10%;">
				
				<script type="text/javascript">
			  		//alert("Hello World!");
				</script>
				
		    <% if (currentUser) { %>
					<p> <%= currentUser.twitterID %> </p>
					<img src="<%= currentUser.picURL %>" width="100">
				<% } %>
				
				<p>Network:</p>
					<%for (var i = 0; i < iPV4Interfaces.length; i++) { %>
						<p> <%= iPV4Interfaces[i].name  %>-<%= iPV4Interfaces[i].family  %>:</p>
						<p> <%= iPV4Interfaces[i].address  %></p>
					<% } %>
				
				<svg width="50" height="50">
				 <rect x="0" y="0" width="50" height="50" fill="green" />
				</svg>
				
				<svg width="50" height="50">
				 <circle cx="25" cy="25" r="25" fill="purple" />
				</svg>
				
				<svg width="50" height="50">
				  <ellipse cx="25" cy="25" rx="15" ry="10" fill="red" />
				</svg>
				
				<svg width="50" height="50">
					<line x1="5" y1="5" x2="40" y2="40" stroke="gray" stroke-width="5"  />
				</svg>
				
				<svg width="50" height="50">
					<polyline fill="none" stroke="blue" stroke-width="2"
				 		points="05,30
				             15,30
				             15,20
				             25,20
				             25,10
				             35,10" />
				 </svg>
				 
				<svg width="50" height="50">
				  <polygon fill="yellow" stroke="blue" stroke-width="2"
				    points="05,30
				            15,10
				            25,30" />
				</svg>
				
			</div>
			
			<!-- Main Column -->
			<div class="column-main" style="float: right; width: 90%;">
				
		    <% if (iFrameURL) { %>
						<p> frame  <%= iFrameURL %> </p>
						<iframe src="<%= iFrameURL %>" width="100%" height="100%" scrolling="no">
				<% } %>
				
				</iframe>
			</div>


		</div>
		</body>
</html>
